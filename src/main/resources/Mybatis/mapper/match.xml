<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.match">
    <select id="postUserMatch" parameterType="join" resultType="int">
        SELECT COUNT(id) FROM test_board WHERE id=#{id} AND name=#{name}
    </select>
</mapper>
